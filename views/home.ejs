  <header>
    <div id="carouselExampleIndicators" style="width: 100%;" class="carousel slide" data-ride="carousel">
      <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
      </ol>
      <div class="carousel-inner" role="listbox">
        <!-- Slide One - Set the background image for this slide in the line below -->
        <div class="carousel-item active" style="background-image: url(/images/funder.jpeg)">
          <div class="row justify-content-center">
            <div class="col-lg-10 col-md-12 col-xs-12">
              <div class="contents col-md-12 text-center">
                <div style="height: 8em;"></div>
                <h2 class="head-title" style="color: aliceblue">As a funder,</h2>
                <h2 class="head-title" style="color: aliceblue">Find the interesting project to support</h2>
                <br>
                <br>
                
                <p style="color: aliceblue">Click find more below to browse more projects</p>
                <div class="col-md-12 text-center">
                  <button class="btn btn-info" type="button" onclick="location.href='/view-projects'">Find More</button>
                </div>
              </div>
            </div> 
          </div>
        </div>
        <!-- Slide Two - Set the background image for this slide in the line below -->
        <div class="carousel-item" style="background-image: url('https://source.unsplash.com/bF2vsubyHcQ/1920x1080')">
          <div class="row justify-content-center">
            <div class="col-lg-10 col-md-12 col-xs-12">
              <div class="contents col-md-12 text-center">
                <div style="height: 8em;"></div>
                <h2 class="head-title" style="color: aliceblue">As a creater,</h2>
                <h2 class="head-title" style="color: aliceblue">Create your project to get the fund</h2>
                <br>
                <br>
                
                <p>Click find more below to browse more projects</p>
                <div class="col-md-12 text-center">
                  <button class="btn btn-info" type="button" onclick="location.href='/createProject'">Create</button>
                </div>
              </div>
            </div> 
          </div>
        </div>
        <!-- Slide Three - Set the background image for this slide in the line below -->
        <div class="carousel-item" style="background-image: url(/images/creater1.jpeg)">
          <div class="row justify-content-center">
            <div class="col-lg-10 col-md-12 col-xs-12">
              <div class="contents col-md-12 text-center">
                <div style="height: 8em;"></div>
                <h2 class="head-title" style="color:aliceblue">As a creater,</h2>
                <h2 class="head-title" style="color: aliceblue">Update your project progress to get more fund</h2>
                <br>
                <br>
                
                <p style="color: rgb(0, 0, 0)e">Click update below to update the progress of your projects</p>
                <div class="col-md-12 text-center">
                  <button class="btn btn-info" type="button" onclick="location.href='/viewCreatedProjects'">Update</button>
                </div>
              </div>
            </div> 
          </div>
        </div>
      </div>
      <div style="position: relative; top:-20em">
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>
